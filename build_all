rm -rf build
mkdir build

xcodebuild -configuration Debug -sdk iphoneos7.1 -project SoomlaiOSCore.xcodeproj -target SoomlaiOSCore clean build CREATING_UNIVERSAL_DIR=$PWD/build/debug
xcodebuild -configuration Release -sdk iphoneos7.1 -project SoomlaiOSCore.xcodeproj -target SoomlaiOSCore clean build CREATING_UNIVERSAL_DIR=$PWD/build/release
